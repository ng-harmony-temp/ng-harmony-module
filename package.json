{
  "name": "ng-harmony-ioc",
  "description": "An AppManager for ng-harmony angular apps",
  "version": "0.1.3",
  "homepage": "https://github.com/ng-harmony/ng-harmony-ioc",
  "author": {
    "name": "Joehannes",
    "email": "johannes.neugschwentner@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ng-harmony/ng-harmony-ioc.git"
  },
  "bugs": {
    "url": "https://github.com/ng-harmony/ng-harmony-ioc/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ng-harmony/ng-harmony-ioc/blob/master/LICENSE"
    }
  ],
  "main": "dist/esnext/ng_harmony.js",
  "engines": {
    "node": ">= 0.10.36",
    "npm": ">=1.4.28"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "literate-programming README.md",
    "beautify": "sed -i 's/^\\s*//' ./dist/raw/ng_harmony.js && js-beautify -m 1 -s 4 -l 0 -p ./dist/raw/ng_harmony.js > ./dist/esnext/ng_harmony.js",
    "transpile_amd": "babel --modules amd -e 1 -s -o ./dist/es5/amd_module.js ./dist/esnext/ng_harmony.js",
    "transpile_common": "babel --modules common -e 1 -s -o ./dist/es5/common_module.js ./dist/esnext/ng_harmony.js",
    "transpile_system": "babel --modules system -e 1 -s -o ./dist/es5/system_module.js ./dist/esnext/ng_harmony.js",
    "transpile_umd": "babel --modules umd -e 1 -s -o ./dist/es5/umd_module.js ./dist/esnext/ng_harmony.js",
    "all": "npm run build && npm run sweeten && npm run beautify && npm run transpile_amd && npm run transpile_common && npm run transpile_system && npm run transpile_umd"
  },
  "dependencies": {
    "angular": "^1.3.15",
    "angular-ui-router": "^0.2.13",
    "ng-harmony": "^0.2.1"
  },
  "devDependencies": {
    "babel": "^5.1.10",
    "js-beautify": "^1.5.5",
    "jspm": "^0.16.11",
    "literate-programming": "^0.8.4"
  },
  "keywords": [],
  "jspm": {
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.4.7",
      "angular-ui-router": "github:angular-ui/ui-router@^0.2.15"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
